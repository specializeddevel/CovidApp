<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.csrabolivia.cuidarnos">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="org.csrabolivia.cuidarnos.AutodiagnosticoPeligrosP2Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.csrabolivia.cuidarnos.MensajeAutodiagnosticoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.csrabolivia.cuidarnos.AutodiagnosticoPeligrosP1Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.csrabolivia.cuidarnos.AutodiagnosticoMolestiasActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.csrabolivia.cuidarnos.AntecedentesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.csrabolivia.cuidarnos.AutodiagnosticoInicialActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.csrabolivia.cuidarnos.PageTwoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.csrabolivia.cuidarnos.PageOneActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.csrabolivia.cuidarnos.AccesoAplicacionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.csrabolivia.cuidarnos.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="org.csrabolivia.cuidarnos.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>